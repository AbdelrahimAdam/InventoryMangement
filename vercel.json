{
  "version": 2,
  "name": "monofia-perfume-inventory",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vue",
  "env": {
    "NODE_ENV": "production",
    "VUE_APP_MODE": "production",
    "NODE_VERSION": "22"
  },
  "github": {
    "silent": true
  }
}