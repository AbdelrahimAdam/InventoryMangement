{
  "version": 2,
  "name": "monofia-perfume-inventory",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci --legacy-peer-deps",
  "framework": "vue",
  "env": {
    "NODE_ENV": "production",
    "VUE_APP_MODE": "production"
  }
}